{
  "name": "react-es6-webpack-boilerplate",
  "version": "4.0.0",
  "description": "Boilerplate for kick starting a React Project with ES6 (Babel) and Hot reloader using Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js & npm run hz",
    "onlyweb": "node server.js",
    "build": "./node_modules/.bin/webpack --config webpack.config.production.js && cp index.html dist",
    "lint": "./node_modules/.bin/eslint .",
    "hz": "./node_modules/.bin/hz serve",
    "deploy": "npm run build && docker build -t houshuang/rethink . && docker push houshuang/rethink && docker $(docker-machine config prod) pull houshuang/rethink && docker $(docker-machine config prod) stop $(docker $(docker-machine config prod) ps -a -q) && docker $(docker-machine config prod) run -d -p 80:8181 houshuang/rethink"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vasanthk/react-es6-webpack-boilerplate.git"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack"
  ],
  "author": "Vasanth Krishnamoorthy (http://www.vasanthk.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasanthk/react-es6-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/vasanthk/react-es6-webpack-boilerplate",
  "dependencies": {
    "async-function-queue": "^1.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.3.19",
    "bootstrap": "^3.3.6",
    "bootstrap-webpack": "0.0.5",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "deep-equal": "^1.0.1",
    "horizon-redux-sync": "^1.0.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "formsy-react": "^0.17.0",
    "formsy-react-components": "^0.6.6",
    "history": "^1.17.x",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.0",
    "json-path": "^0.1.3",
    "jsxstyle": "0.0.18",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "lodash": "^4.2.1",
    "material-ui": "^0.14.4",
    "normalize.css": "^3.0.3",
    "rethinkdb": "2.2.3",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-draggable": "houshuang/react-draggable",
    "react-icons": "^1.0.3",
    "react-json-inspector": "^6.1.2",
    "react-modal": "^0.6.1",
    "react-redux": "^4.2.1",
    "react-router": "^2.0.0-rc6",
    "react-tap-event-plugin": "^0.2.2",
    "react-toolbox": "^0.14.1",
    "redux": "^3.2.1",
    "redux-form": "^4.1.10",
    "url-loader": "^0.5.7",
    "xtend": "^4.0.1",
    "horizon": "^0.3.1",
    "encore_login": "0.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.19",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-runtime": "^6.3.19",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0",
    "expose-loader": "^0.7.1",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.8.1",
    "node-sass": "^3.4.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "sass-loader": "^3.1.2",
    "style-loader": "0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.8.0",
    "webpack-hot-middleware": "^2.6.4",
    "yargs": "^3.32.0"
  }
}
